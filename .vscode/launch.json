{
    "version": "0.2.0",
    "configurations": [
      {
        "type": "node",
        "request": "launch",
        "name": "Debugger",
        "runtimeExecutable": "npm",
        "runtimeArgs": ["run-script", "start:dev"],
        "skipFiles": ["<node_internals>/**"],
